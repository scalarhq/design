{
  "name": "@modfy/design",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@vitejs/plugin-react": "^2.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@cryogenicplanet/tsdev": "latest",
    "@tailwindcss/forms": "^0.5.3",
    "@twind/forms": "latest",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "latest",
    "dts-bundle": "latest",
    "husky": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "prettier-config-standard": "latest",
    "tailwindcss": "latest",
    "twind": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "latest"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "dts": "tsdev dts",
    "lint": "tsdev lint",
    "prepare": "pnpm build",
    "start": "vite preview"
  },
  "files": [
    "dist",
    "!dist/src"
  ],
  "engines": {
    "node": ">12"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "tsdev prettier",
      "pre-push": "tsdev lint"
    }
  },
  "version": "0.1.5",
  "license": "MIT",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./withModfyTailwindConfig": {
      "require": "./dist/tailwind.config.cjs.js",
      "import": "./dist/tailwind.config.es.js"
    }
  },
  "prettier": "prettier-config-standard",
  "tsdev": {
    "template": 3,
    "packageManager": 2,
    "tailwindcss": true
  },
  "description": "Here are where we stores custom tailwind properties, colors, etc.",
  "author": ""
}
